<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1">
   <center>
      <TableView prefHeight="200.0" prefWidth="538.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="133.0" text="이름" fx:id="name" />
          <TableColumn prefWidth="138.0" text="국어" fx:id="korean" />
            <TableColumn prefWidth="136.0" text="수학" fx:id="math" />
            <TableColumn prefWidth="130.0" text="영어" fx:id="english" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <HBox prefHeight="66.0" prefWidth="538.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="add" layoutX="80.0" layoutY="100.0" mnemonicParsing="false" text="추가">
               <HBox.margin>
                  <Insets left="250.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
